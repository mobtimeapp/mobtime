{
  "name": "mobtime",
  "version": "1.3.0",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrozbarry/mobtime.git"
  },
  "scripts": {
    "migrate:latest": "knex --esm migrate:latest",
    "migrate:up": "knex --esm migrate:up",
    "migratedownp": "knex --esm migrate:down",
    "dev": "nodemon --ignore public ./index.js",
    "start": "node ./index.js",
    "live": "echo 'This is deprecated, use yarn start now'; yarn start'",
    "lint": "eslint .",
    "deploy": "node ./deploy/index.js",
    "test": "ava"
  },
  "dependencies": {
    "@mrbarrysoftware/js-enumerize": "^1.1.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "ferp": "^1.2.0",
    "helmet": "^3.21.3",
    "knex": "^0.20.13",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.2",
    "semver": "^7.1.3",
    "sqlite3": "^4.1.1",
    "ws": "^7.2.3"
  },
  "private": true,
  "devDependencies": {
    "ava": "^3.8.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "resolve": "^1.15.1"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "concurrency": 8,
    "failFast": true,
    "failWithoutAssertions": true,
    "require": [
      "esm"
    ],
    "verbose": true
  }
}
